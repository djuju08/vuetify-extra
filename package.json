{
  "name": "@menteora/vuetify-extra",
  "version": "0.2.7",
  "scripts": {
    "serve": "vue-cli-service serve ./dev/main.js",
    "build:demo": "vue-cli-service build --dest demo ./dev/main.js",
    "build": "vue-cli-service build --target lib --name menteora src/plugin.js",
    "build:local": "vue-cli-service build --target lib --name menteora src/plugin.js && npm pack",
    "lint": "vue-cli-service lint --fix && vue-cli-service lint dev --fix",
    "deploy:demo": "vue-cli-service build --dest demo ./dev/main.js && ./deploy.demo.sh"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/menteora.common.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/menteora/vuetify-extra"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-eslint": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify": "^2.0.4",
    "vue-json-tree-view": "^2.1.4",
    "vue-json-tree": "^0.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.11",
    "vuetify-loader": "^1.4.3"
  }
}
